{
  "name": "acspl",
  "displayName": "Acs-Code-Highlight",
  "description": "ACS Controller Code Highlight",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.62.0"
  },
  "keywords": [
    "acspl+",
    "acs",
    "acs motion control",
    "spiiplus"
  ],
  "icon": "images/acs.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "acspl",
        "aliases": [
          "ACSPL+",
          "acspl"
        ],
        "extensions": [
          ".prg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "acspl",
        "scopeName": "source.prg",
        "path": "./syntaxes/acspl.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}